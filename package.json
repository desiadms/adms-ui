{
  "name": "adms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "spit-graphql-schema": "bin/fetch-schema.sh",
    "generate-all-types": "graphql-codegen --config codegen.ts",
    "dev:generate-all-types": "graphql-codegen --config codegen.ts --watch",
    "dev:vite": "vite",
    "install-deps": "bun i",
    "dev:netlify-server": "netlify dev",
    "watch": "run-p dev:*",
    "dev": "run-s clean install-deps spit-graphql-schema generate-all-types watch",
    "build": "bun spit-graphql-schema && bun generate-all-types && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,scss}\" --write",
    "test": "vitest",
    "playwright:test": "bun playwright test",
    "playwright:test:ui": "bun playwright test --ui",
    "test:e2e": "dotenv-run-script playwright:test",
    "test:e2e:ci": "bun playwright install && bun playwright install-deps && bun playwright:test",
    "test:e2e:ui": "dotenv-run-script playwright:test:ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
    "@nhost/react": "^3.5.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-router": "^1.22.3",
    "@yudiel/react-qr-scanner": "^2.0.4",
    "axios": "^1.6.8",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.5.1",
    "graphql": "^16.8.1",
    "idb-keyval": "^6.2.1",
    "jotai": "^2.7.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-hot-toast": "^2.4.1",
    "remeda": "^1.56.0",
    "rxdb": "^15.12.0",
    "rxdb-hooks": "^5.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-inspector/cli": "^4.0.3",
    "@parcel/watcher": "^2.4.1",
    "@playwright/test": "^1.42.1",
    "@tanstack/router-devtools": "^1.22.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "codegen": "^0.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^6.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^3.1.0",
    "graphql-codegen-typescript-validation-schema": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.4",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-pwa": "^0.18.2",
    "vite-plugin-restart": "^0.4.0",
    "vitest": "^1.4.0"
  }
}
