{
  "name": "adms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run fetch-graphql-schema && concurrently \"vite\" \"yarn generate --watch\"",
    "hasura-introspect": "gq $VITE_HASURA_ENDPOINT -H \"X-Hasura-Admin-Secret: $HASURA_ADMIN_SECRET\" --introspect > schema.graphql",
    "fetch-graphql-schema": "dotenv-run-script .env -- hasura-introspect",
    "generate": "graphql-codegen",
    "build": "tsc && yarn fetch-graphql-schema && yarn generate && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,scss}\" --write",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@nhost/react": "^2.0.19",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-query-persist-client": "^4.29.5",
    "@tanstack/router": "0.0.1-beta.84",
    "@tanstack/router-devtools": "0.0.1-beta.84",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "concurrently": "^8.0.1",
    "console-feed": "^3.5.0",
    "dotenv-run-script": "^0.2.0",
    "graphql": "^16.6.0",
    "idb-keyval": "^6.2.0",
    "jotai": "^2.1.0",
    "preact": "^10.13.1",
    "qrcode.react": "^3.1.0",
    "react-hook-form": "^7.44.1",
    "react-hot-toast": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@preact/preset-vite": "^2.5.0",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphqurl": "^1.0.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.23",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-restart": "^0.3.1",
    "vitest": "^0.31.0"
  }
}
