{
  "name": "adms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"bun generate --watch\"",
    "dev:ci": "concurrently \"vite\" \"bun generate --watch\"",
    "hasura-introspect": "echo $VITE_HASURA_NAME && bin/fetch-schema.sh",
    "codegen": "graphql-codegen --config codegen.ts",
    "introspect": "bun run hasura-introspect",
    "dev:codegen": "sleep 10 && graphql-codegen --config codegen.ts --watch",
    "fetch-graphql-schema": "dotenv-run-script .env -- hasura-introspect",
    "foo": "echo $HASURA_ADMIN_SECRET && bin/fetch-schema.sh",
    "dev:load-env": "dotenv-run-script .env -- foo",
    "generate": "graphql-codegen",
    "build": "tsc && bun hasura-introspect && bun generate && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,scss}\" --write",
    "prepare": "husky install",
    "test": "vitest",
    "playwright:test": "bun playwright test",
    "playwright:test:ui": "bun playwright test --ui",
    "test:e2e": "dotenv-run-script playwright:test",
    "test:e2e:ci": "bun playwright install && bun playwright install-deps && bun playwright:test",
    "test:e2e:ui": "dotenv-run-script playwright:test:ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@nhost/react": "^2.0.26",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/router": "0.0.1-beta.84",
    "@tanstack/router-devtools": "0.0.1-beta.84",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "concurrently": "^8.0.1",
    "dotenv-run-script": "^0.2.0",
    "graphql": "^16.6.0",
    "idb-keyval": "^6.2.0",
    "jotai": "^2.1.0",
    "preact": "^10.13.1",
    "qrcode.react": "^3.1.0",
    "react-hook-form": "^7.44.1",
    "react-hot-toast": "^2.4.1",
    "rxdb": "^14.15.1",
    "rxdb-hooks": "^5.0.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.2",
    "@graphql-codegen/introspection": "^4.0.2",
    "@parcel/watcher": "^2.4.0",
    "@playwright/test": "latest",
    "@preact/preset-vite": "^2.5.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "codegen": "^0.1.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql-codegen-typescript-validation-schema": "^0.12.1",
    "graphql-inspector": "^0.5.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^4.3.2",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-restart": "^0.3.1",
    "vitest": "^0.31.0"
  }
}
