{
  "name": "adms-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "spit-graphql-schema": "bin/fetch-schema.sh",
    "generate-all-types": "graphql-codegen --config codegen.ts",
    "dev:generate-all-types": "graphql-codegen --config codegen.ts --watch",
    "dev:vite": "vite",
    "dev:netlify-server": "netlify dev",
    "watch": "run-p dev:*",
    "dev": "run-s clean spit-graphql-schema generate-all-types watch",
    "build": "tsc && bun spit-graphql-schema && bun generate-all-types && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,scss}\" --write",
    "test": "vitest",
    "playwright:test": "bun playwright test",
    "playwright:test:ui": "bun playwright test --ui",
    "test:e2e": "dotenv-run-script playwright:test",
    "test:e2e:ci": "bun playwright install && bun playwright install-deps && bun playwright:test",
    "test:e2e:ui": "dotenv-run-script playwright:test:ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@nhost/react": "^3.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-router": "^1.16.6",
    "axios": "^1.6.7",
    "classnames": "^2.3.2",
    "concurrently": "^8.0.1",
    "graphql": "^16.6.0",
    "idb-keyval": "^6.2.0",
    "jotai": "^2.6.4",
    "preact": "^10.19.4",
    "qrcode.react": "^3.1.0",
    "react-hook-form": "^7.44.1",
    "react-hot-toast": "^2.4.1",
    "remeda": "^1.44.0",
    "rxdb": "^15.8.0",
    "rxdb-hooks": "^5.0.2",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.2",
    "@graphql-codegen/introspection": "^4.0.2",
    "@parcel/watcher": "^2.4.0",
    "@playwright/test": "^1.41.2",
    "@preact/preset-vite": "^2.5.0",
    "@tanstack/router-devtools": "^1.16.6",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "codegen": "^0.1.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql-codegen-typescript-validation-schema": "^0.12.1",
    "graphql-inspector": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-pwa": "^0.18.1",
    "vite-plugin-restart": "^0.3.1",
    "vitest": "^1.2.2"
  }
}
